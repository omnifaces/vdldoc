<?xml version='1.0' encoding='UTF-8'?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://vdldoc.org/vdldoc" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:element name="deprecated">
